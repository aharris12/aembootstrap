<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:scg="http://www.adobe.com/social/scg/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:description="Template with focus on content."
          jcr:primaryType="cq:Template"
          jcr:title="AEM Bootstrap Content Page"
          allowedPaths="[/content/aembootstrap/.+]"
          ranking="{Long}100">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="AEMBootstrap/components/structure/page">
        <logo
            jcr:primaryType="nt:unstructured"
            sling:resourceType="AEMBootstrap/components/structure/logo"/>
        <title
            jcr:primaryType="nt:unstructured"
            sling:resourceType="AEMBootstrap/components/content/title"/>
        <topnav
            jcr:primaryType="nt:unstructured"
            sling:resourceType="AEMBootstrap/components/structure/topnav"/>
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="AEMBootstrap/components/content/text"
                text="&lt;p>This is a content page&lt;/p>"
                textIsRich="true"/>
            <clientlibslist
                    cq:lastReplicated="{Date}2016-01-15T22:49:06.470+05:30"
                    cq:lastReplicatedBy="admin"
                    cq:lastReplicationAction="Activate"
                    jcr:created="{Date}2013-12-06T13:01:47.362-05:00"
                    jcr:createdBy="admin"
                    jcr:lastModified="{Date}2013-12-06T13:10:44.759-05:00"
                    jcr:lastModifiedBy="admin"
                    jcr:mixinTypes="[cq:ReplicationStatus]"
                    jcr:primaryType="nt:unstructured"
                    scg:requiredClientLibs="apps.acme.ideation, cq.social.hbs.voting, cq.ckeditor"
                    sling:resourceType="social/console/components/clientlibslist"/>
        </par>
    </jcr:content>
</jcr:root>
